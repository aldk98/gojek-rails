<p id="notice"><%= notice %></p>

<h1>Makanans</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @makanans.each do |makanan| %>
      <tr>
        <td><%= link_to 'Show', makanan %></td>
        <td><%= link_to 'Edit', edit_makanan_path(makanan) %></td>
        <td><%= link_to 'Destroy', makanan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Makanan', new_makanan_path %>

<p id="notice"><%= notice %></p>

<h1>Minumen</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @minumen.each do |minuman| %>
      <tr>
        <td><%= link_to 'Show', minuman %></td>
        <td><%= link_to 'Edit', edit_minuman_path(minuman) %></td>
        <td><%= link_to 'Destroy', minuman, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Minuman', new_minuman_path %>
